new {{variantNamespace}}.Element(gameObject, this, "{{ElementConstruct.Name}}")  
{
    Elements = new {{variantNamespace}}.Element[]
    {
    {%- Indent false -%}
    {% for childElement in ElementConstruct.Elements -%}
    {% include 'ElementConstruct' with childElement %}{% if forloop.last == false %},{% endif %}
    {% endfor -%}             
    {%- endIndent -%}    
    }
}